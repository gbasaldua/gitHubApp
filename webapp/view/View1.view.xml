<mvc:View controllerName="gitHubApp.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<customHeader>
					<Toolbar>
						<Button type="Back" press="onPress"/>
						<ToolbarSpacer/>
						<Title text="{i18n>title}" level="H2"/>
						<ToolbarSpacer/>
						<Button icon="sap-icon://edit" type="Transparent" press="onPress" ariaLabelledBy="editButtonLabel"/>
					</Toolbar>
				</customHeader>
				<content>
					<HBox>
						<Button text="Default" press="onPress">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Button>
						<Button type="Accept" text="Accept" press="onPress">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Button>
						<Button type="Reject" text="Reject" press="onPress">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Button>
						<Button text="Coming Soon" press="onPress" enabled="false">
							<layoutData>
								<FlexItemData growFactor="1"/>
							</layoutData>
						</Button>
					</HBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>